#include <iostream>
using namespace std;

int main() {

	float x0,x1,x2,x3,x4,x5,x6,x7,x8,x9,sum,sumSquares;

	cout << "Type 10 integers, each separated by space: 1 2 3...10" << endl;
	cin >> x0;
	cin >> x1;
	cin >> x2;
	cin >> x3;
	cin >> x4;
	cin >> x5;
	cin >> x6;
	cin >> x7;
	cin >> x8;
	cin >> x9;

	sum = x0+x1+x2+x3+x4+x5+x6+x7+x8+x9;
	sumSquares = x0^2+x1^2+x2^2+x3^2+x4^2+x5^2+x6^2+x7^2+x8^2+x9^2;

	cout << sum;

	return 0;

}



